<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="iOS test suite on Anroid RealPhone">

    <parameter name="OS" value="Android"/>
    <parameter name="appType" value="Phone"/>
    <parameter name="deviceType" value="RealDevice"/>
    <parameter name="deviceName" value="Galaxy Note8"/>
    <parameter name="version" value="7.1.1"/>

    <test name="NewYorkPost Pages Test" preserve-order="true">
        <classes>
            <class name="testPages.HomePageTest"/>
           <class name="testPages.BreakingNewsPageTest"/>
            <class name="testPages.BusinessTest"/>
            <class name="testPages.EntertainmentTest"/>
            <class name="testPages.FashionTest"/>
            <class name="testPages.MediaTest"/>
            <class name="testPages.MenuPageTest"/>
            <class name="testPages.OpinionTest"/>
            <class name="testPages.SportsEventTest"/>
        </classes>
    </test>
</suite>
