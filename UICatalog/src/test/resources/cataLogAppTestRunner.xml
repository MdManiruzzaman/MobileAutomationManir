           <class name="testCasesIPhone.TestButton"/>
            <class name="testCasesIPhone.TestControls"/>
            <class name="testCasesIPhone.TestImages"/>
            <class name="testCasesIPhone.TestTextFields"/>
            <class name="testCasesIPhone.TestPicker"/>
            <class name="testCasesIPhone.TestSegments"/>
            <class name="testCasesIPhone.TestToolbar"/>
            <class name="testCasesIPhone.TestWeb"/>
