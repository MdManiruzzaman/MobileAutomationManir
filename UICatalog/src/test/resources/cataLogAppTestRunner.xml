<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="iOS test suite on iPhoneTest Simulator">

    <parameter name="OS" value="ios"/>
    <parameter name="appType" value="iPhoneTest"/>
    <parameter name="deviceType" value="Simulator"/>
    <parameter name="deviceName" value="iPhoneTest Simulator"/>
    <parameter name="version" value="10.3"/>

    <test name="iPhoneTest" preserve-order="true">
        <classes>
            <class name="iPhoneTest.TestAlerts"/>
           <class name="iPhoneTest.TestButton"/>
            <class name="iPhoneTest.TestControls"/>
            <class name="iPhoneTest.TestTextFields"/>
            <class name="iPhoneTest.TestPicker"/>
            <class name="iPhoneTest.TestSegments"/>
            <class name="iPhoneTest.TestToolbar"/>
            <class name="iPhoneTest.TestWeb"/>
        </classes>
    </test>
</suite>
